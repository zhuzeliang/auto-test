{
  "name": "auto-test",
  "version": "1.0.0",
  "description": "自动化测试平台",
  "main": "app.js",
  "scripts": {
    "test": "node test/gen && cross-env NODE_ENV=test jest --runInBand --forceExit",
    "test:watch": "npm run test -- --watch",
    "test:cover": "npm run test -- --coverage",
    "lint": "eslint --ignore-path .gitignore . --quiet --ext .vue,.js",
    "dev": "nodemon --ignore views/ --ignore public/ app",
    "dev:client": "node app",
    "dev:server": "cross-env NODE_ENV=production npm run dev",
    "start": "cross-env NODE_ENV=production node app",
    "build": "cross-env NODE_ENV=production npm run build:dev",
    "build:dev": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "webpack --config build/webpack.server.config.js --progress --hide-modules"
  },
  "author": "zhuzeliang",
  "license": "GPL-3.0",
  "keywords": [
    "auto-test",
    "test"
  ],
  "dependencies": {
    "@koa/cors": "^2.2.1",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "brace": "^0.11.0",
    "config": "^1.29.2",
    "cross-env": "^5.1.3",
    "ip-filter": "^2.0.0",
    "koa": "^2.4.1",
    "koa-body": "^2.5.0",
    "koa-favicon": "^2.0.0",
    "koa-jwt": "^3.3.0",
    "koa-onerror": "^4.0.0",
    "koa-pino-logger": "^2.1.1",
    "koa-ratelimit": "^4.0.0",
    "koa-router": "^7.3.0",
    "koa-static-cache": "^5.1.1",
    "koa-validate": "^1.0.7",
    "lodash": "^4.17.4",
    "lru-cache": "^4.1.1",
    "mkdirp": "^0.5.1",
    "moment": "^2.20.1",
    "mongoose": "^4.13.9",
    "path-to-regexp": "^2.1.0",
    "pino-multi-stream": "^3.1.2",
    "restc": "^0.2.2",
    "rimraf": "^2.6.2",
    "swagger-parser-mock": "^1.1.1",
    "universal-cookie": "^2.1.2",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "codecov": "^3.0.0",
    "css-loader": "^0.28.8",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "jest": "^22.1.1",
    "koa-webpack-middleware": "^1.0.7",
    "nodemon": "^1.14.11",
    "precss": "^2.0.0",
    "raw-loader": "^0.5.1",
    "saladcss-bem": "^0.0.2",
    "supertest": "^3.0.0",
    "transliteration": "^1.6.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-merge": "^4.1.1",
    "webpack-node-externals": "^1.6.0"
  },
  "optionalDependencies": {
    "fsevents": "*"
  }
}
